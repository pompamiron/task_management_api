{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "globals": {
        "module": "readonly"
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "project": "./tsconfig.json",
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off"
    },
    "plugins": ["prettier"]
}
